{
  "update": [
    {
      "fileName": "app-routes.js",
      "rules": [
        {
          "before": "import",
          "after": "<%=^empty%>import"
        },
        {
          "before": "];",
          "after": "<%=/empty%>];"
        },
        {
          "before": "export default [",
          "after": "<%=/empty%>export default [<%=^empty%>"
        }
      ]
    },
    {
      "fileName": "app-navigation.js",
      "rules": [
        {
          "before": "= [",
          "after": "= [<%=^empty%>"
        },
        {
          "before": "];",
          "after": "<%=/empty%>];"
        }
      ]
    },
    {
      "fileName": "app-info.js",
      "rules": [
        {
          "before": "My App",
          "after": "<%=project%>"
        }
      ]
    },
    {
      "fileName": "App.js",
      "rules": [
        {
          "before": "SideNavOuterToolbar",
          "after": "<%=layout%>"
        },
        {
          "before": "SideNavInnerToolbar",
          "after": "<%=layout%>"
        },
        {
          "before": "))}",
          "after": "))}<%=^empty%>"
        },
        {
          "before": "<Redirect to={'/home'} />",
          "after": "<Redirect to={'/home'} /><%=/empty%>"
        }
      ]
    }
  ],
  "replace": [
    {
      "from": "pages\\",
      "to": "templates\\react\\sample-pages\\"
    }
  ],
  "ignore": [
    "themes",
    "App.css",
    "App.test.js",
    "logo.svg",
    "serviceWorker.js",
    "setupTests.js",
    "index.css",
    "index.js"
  ]
}
