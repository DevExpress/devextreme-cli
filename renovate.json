{
  "extends": [
    "config:base"
  ],
  "baseBranches": [
    "master"
  ],
  "timezone": "America/Los_Angeles",
  "lockFileMaintenance": {
    "enabled": true,
    "schedule": [
      "every 1 day"
    ],
    "automerge": true
  },
  "vulnerabilityAlerts": {
    "enabled": true
  },
  "packageRules": [
    {
      "packagePatterns": [
        "*"
      ],
      "enabled": false
    }
  ],
  "ignorePaths": [
    ".github"
  ]
}