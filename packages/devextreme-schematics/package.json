{
  "name": "devextreme-schematics",
  "version": "1.7.2",
  "description": "DevExtreme schematics",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "lint": "tslint -c tslint.json 'src/*/*.ts' -e '**/*.d.ts'",
    "test": "npm run lint && npm run build && jasmine **/*_spec.js"
  },
  "author": "Developer Express Inc.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/DevExpress/devextreme-cli/tree/master/packages/devextreme-schematics"
  },
  "homepage": "https://www.npmjs.com/package/devextreme-schematics",
  "files": [
    "src/**/*",
    "!*.ts",
    "*.d.ts",
    "src/*/files/**/*.ts"
  ],
  "schematics": "./src/collection.json",
  "dependencies": {
    "@angular-devkit/core": "^16.2.10",
    "@angular-devkit/schematics": "^16.2.10",
    "@schematics/angular": "^16.2.10"
  },
  "devDependencies": {
    "@types/jasmine": "~3.10.18",
    "@types/node": "ts4.9",
    "@types/semver": "^7.5.7",
    "jasmine": "^2.99.0",
    "rxjs": "^6.6.7",
    "tslint": "^5.20.1",
    "typescript": "^4.9.3"
  }
}
