{
  "name": "devextreme-cli",
  "version": "1.1.12",
  "description": "DevExtreme CLI",
  "keywords": [
    "devexpress",
    "devextreme",
    "cli",
    "themebuilder",
    "theme",
    "application"
  ],
  "bin": {
    "devextreme": "index.js"
  },
  "files": [
    "commands/**/*",
    "templates/**/*",
    "utility/**/*",
    "cli-config.json",
    "index.js"
  ],
  "scripts": {
    "lint": "eslint ./",
    "test": "npm run lint && jest --runInBand",
    "test-dev": "cross-env TEST_MODE=dev jest",
    "docker:build": "docker build --rm -f \"testing/docker/Dockerfile\" -t devextreme-cli:latest \"testing/docker\"",
    "docker:run": "docker run --rm -d  -p 9222:9222/tcp devextreme-cli:latest"
  },
  "author": "Developer Express Inc.",
  "repository": "https://github.com/DevExpress/devextreme-cli",
  "license": "MIT",
  "dependencies": {
    "@vue/cli": "^3.5.5",
    "ip": "^1.1.5",
    "less": "3.9.0",
    "minimist": "^1.2.0",
    "mustache": "^3.0.1",
    "node-sass": "^4.9.3",
    "prompts": "^2.0.4",
    "semver": "^5.6.0"
  },
  "devDependencies": {
    "cross-env": "^5.2.0",
    "eslint": "^5.10.0",
    "eslint-plugin-jest": "^22.5.1",
    "jest": "^24.9.0",
    "jest-image-snapshot": "^2.8.2",
    "puppeteer": "^2.1.1",
    "rimraf": "^2.6.3",
    "tree-kill": "^1.2.1"
  }
}
